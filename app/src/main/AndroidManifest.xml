<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.febrian.simpleapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/poster_aquaman"
        android:label="@string/app_name"
        android:roundIcon="@drawable/poster_aquaman"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimpleApp">
        <activity android:name=".FisikaActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".KonversiActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>